{
    "compilerOptions": {
      "module": "amd",
      "noImplicitAny": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "jsx": "react",
      "jsxFactory": "tsx",
      "target": "es5",
      "experimentalDecorators": true,
      "preserveConstEnums": true,
      "suppressImplicitAnyIndexErrors": true,
      "outDir": "./public/js"
    },
    "include": [
      "./src/*",
      "./src/**/*"
    ],
    "exclude": [
      "node_modules"
    ]
  }